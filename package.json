  },
  "homepage": "https://github.com/chantouchsek/validator-js#readme",
  "optionalDependencies": {
    "lodash.isboolean": "^3.0.3",
    "lodash.isfunction": "^3.0.9",
    "lodash.isnan": "^3.0.2",
    "lodash.isnull": "^3.0.0",
    "lodash.isnumber": "^3.0.3",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
